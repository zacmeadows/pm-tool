<h1>All Projects</h1>

<% @projects.each do |p| %> 
   <p>Title: <%= p.title %></p>
   <p>Description: <%= p.description %></p>
   <p>Due Date: <%= p.due_date %></p>
   <p><%= link_to "Edit", edit_project_path(p) %></p>
   <p><%= link_to "Delete", project_path(p), method: :delete, data: {confirm: "Are you sure?"} %></p>
   <hr>
 <% end %>